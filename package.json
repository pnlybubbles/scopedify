{
  "name": "scopedify",
  "version": "1.0.1",
  "description": "Scoped modular CSS bundler",
  "license": "MIT",
  "scripts": {
    "deps": "dependency-check . --entry transform.js . && dependency-check . --entry transform.js --extra --no-dev -i insert-css",
    "test": "standard && npm run deps && tape test/index.js",
    "test:cov": "standard && npm run deps && NODE_ENV=test istanbul cover test/index.js",
    "format": "standard --format"
  },
  "browser": {
    "index.js": "./browser.js"
  },
  "keywords": [
    "modular",
    "css",
    "bundle",
    "browserify",
    "css-modules"
  ],
  "dependencies": {
    "falafel": "^1.2.0",
    "insert-css": "^2.0.0",
    "map-limit": "0.0.1",
    "postcss": "^5.0.10",
    "postcss-bind-attr": "^1.0.0",
    "resolve": "^1.1.7",
    "stack-trace": "0.0.9",
    "static-eval": "^1.1.0",
    "style-resolve": "^1.0.0",
    "through2": "^2.0.0",
    "xtend": "^4.0.1"
  },
  "devDependencies": {
    "browserify": "^13.0.0",
    "codecov.io": "^0.1.6",
    "concat-stream": "^1.5.1",
    "css-extract": "^1.1.2",
    "css-type-base": "^1.0.2",
    "css-wipe": "^4.2.2",
    "dependency-check": "^2.5.1",
    "domify": "^1.4.0",
    "from2-string": "^1.1.0",
    "istanbul": "^0.4.5",
    "jsdom": "^9.4.2",
    "npm-check-updates": "^2.2.0",
    "rimraf": "^2.5.1",
    "sheetify-cssnext": "^1.0.0",
    "standard": "^8.0.0",
    "tape": "^4.2.0"
  }
}
